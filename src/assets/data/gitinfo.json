{
    "info" : "Git Cheat Sheet",
    "type" : "Cheat Sheet",
    "description" : "Git common commands cheat sheet",
    "ver" : "1.0.0",
    "contents" : [
        {
            "info" : "Configure Tooling",
            "type" : "tooling",
            "description" : "Configure user information for all local repositories",
            "contents" : [
                {
                    "info" : "$ git config --global user.name \"[name]\" ",
                    "type" : "config",
                    "description" : "Sets the name you want attached to your commit transactions"
                },
                {
                    "info" : "$ git config --global user.email \"[email address]\" ",
                    "type" : "config",
                    "description" : "Sets the email you want attached to your commit transactions"
                },
                {
                    "info" : "$ git config --global color.ui auto ",
                    "type" : "config",
                    "description" : "Enables helpful colorization of command line output"
                }
            ]
        }
    ]
}